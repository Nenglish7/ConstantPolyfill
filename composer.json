{
    "name": "nenglish7/constant-polyfill",
    "type": "library",
    "keywords": [
        "php5-6",
        "php7-0",
        "php7-1",
        "php7-2",
        "php-library",
        "constant",
        "polyfill"
    ],
    "description": "A complete polyfill for all the php constants. Be aware we do not support anything before php 5.6. If i missed a constant feel free to point it out.",
    "homepage": "https://github.com/Nenglish7/ConstantPolyfill",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/Nenglish7"
        }
    ],
    "autoload": {
        "files": [
            "polyfill.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5|^6"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
